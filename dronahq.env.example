# DronaHQ Environment File
BUILDER_URL=http://localhost

MYSQL_HOST=dronahq-self-hosted-mysqldb
MYSQL_USER=dronahq
MYSQL_PASSWORD=<random password>

MONGODB_HOST=dronahq-self-hosted-mongodb
MONGODB_USER=dronahq
MONGODB_PASSWORD=<random password>

# (mandatory for db container)Default variables required for Mongo and MySQL container
MYSQL_ROOT_PASSWORD=<random password>
MYSQL_DATABASE=db5x

MONGO_INITDB_ROOT_USERNAME=root
MONGO_INITDB_ROOT_PASSWORD=<random password>
MONGO_INITDB_DATABASE=db5x_studio

# Your dronahq license key here
LICENSE_KEY=<your license key>

